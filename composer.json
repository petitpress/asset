{
	"name": "68publishers/asset",
	"description": "Integration of Symfony's Asset component into Nette Framework.",
	"keywords": ["68publishers", "asset", "nette"],
	"license": "MIT",
	"authors": [
		{
			"name": "Tomáš Glawaty",
			"email": "tomasglawaty@icloud.com"
		}
	],
	"require": {
		"php": "^8.0",
		"nette/di": "^3.0.10",
		"nette/utils": "^3.0",
		"latte/latte": "^3.0",
		"symfony/asset": "^5.0 || ^6.0"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.4",
		"nette/application": "^3.0.6",
		"nette/tester": "^2.3.4",
		"roave/security-advisories": "dev-latest",
		"scrutinizer/ocular": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"SixtyEightPublishers\\Asset\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SixtyEightPublishers\\Asset\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
